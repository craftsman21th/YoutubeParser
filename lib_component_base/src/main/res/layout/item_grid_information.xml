<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_gravity="center"
             android:layout_margin="10dp">
    <com.moder.compass.ui.widget.CircleImageView
            android:layout_width="@dimen/new_dialog_three_image_size"
            android:layout_height="@dimen/new_dialog_three_image_size"
            android:layout_gravity="center"
            android:scaleType="centerCrop"
            android:id="@+id/image"
            app:view_border_color="#00000000"
            app:view_border_width="0dp"
            android:src="@drawable/default_user_head_icon"/>
</FrameLayout>