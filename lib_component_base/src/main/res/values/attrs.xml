    <declare-styleable name="GradientTextView">
        <attr name="selected_start_color" format="color"/>
        <attr name="selected_end_color" format="color"/>
        <attr name="unselected_end_color" format="color"/>
        <attr name="unselected_start_color" format="color"/>
        <attr name="select_state" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SearchBox">
        <attr name="show_clear_icon" format="boolean"/>
    </declare-styleable>
</resources>